@{
    Html.Partial("_Master.Browser");
}
<!DOCTYPE html>
<html @Html.AppPageCustom(PageLayoutPosition.HtmlAttribute)>
<head>
    <meta charset="utf-8">
    <title>@Html.AppPageTitle(true)</title>
    <meta name="description" content="@(Html.AppPageMetaDescription())">
    <meta name="keywords" content="@(Html.AppPageMetaKeyword())">
    @Html.AppPageCustom(PageLayoutPosition.Head, true)
    @Html.AppPageCanonicalUrl()
    @Html.AppPageCss(PageLayoutPosition.Head, Url, true)
    @Html.AppPageScript(PageLayoutPosition.Head, Url, true)
</head>
<body @Html.AppPageCustom(PageLayoutPosition.BodyAttribute)>
<!--[if lt IE 8]>
    <p class="browserupgrade">You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade your browser</a> to improve your experience.</p>
<![endif]-->

@RenderBody()

@Html.AppPageCss(PageLayoutPosition.Body, Url)
@Html.AppPageScript(PageLayoutPosition.Body, Url)

<script>
    (function(b, o, i, l, e, r) {
        b.GoogleAnalyticsObject = l;
        b[l] ||
        (b[l] =
            function() { (b[l].q = b[l].q || []).push(arguments) });
        b[l].l = +new Date;
        e = o.createElement(i);
        r = o.getElementsByTagName(i)[0];
        e.src = 'https://www.google-analytics.com/analytics.js';
        r.parentNode.insertBefore(e, r);
    }(window, document, 'script', 'ga'));
    ga('create', 'UA-48864026-1', 'auto');
    ga('send', 'pageview');
</script>
</body>
</html>